<table class="details">
  <tr>
    <th>[% l('Releases:') %]</th>
    <td colspan="2">
      <ul>
        [% FOR release=edit.display_data.releases %]
        <li>[% descriptive_link(release) %]</li>
        [% END %]
      </ul>
    </td>
  </tr>
  <tr>
    <th>[% l('Release group:') %]</th>
    <td><span class="old">[% descriptive_link(edit.display_data.release_group.old) %]</span></td>
    <td><span class="new">[% descriptive_link(edit.display_data.release_group.new) %]</span></td>
  </tr>
</table>
